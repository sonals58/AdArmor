{
    "manifest_version": 3,
    "name": "AdArmor",
    "version": "1.0.0",
    "permissions": [
        "declarativeNetRequest"
    ],
    "action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    },
    
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}